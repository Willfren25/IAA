# LLM Configuration
LLM_PROVIDER=openai
LLM_API_KEY=your_api_key_here
LLM_MODEL=gpt-4
LLM_TEMPERATURE=0.3
LLM_MAX_TOKENS=2000

# OpenAI Specific (if using OpenAI)
OPENAI_API_KEY=your_openai_key_here
OPENAI_ORG_ID=

# Anthropic Specific (if using Anthropic)
ANTHROPIC_API_KEY=your_anthropic_key_here

# n8n Configuration
N8N_VERSION=1.0.0
N8N_SCHEMA_VERSION=latest

# Application
NODE_ENV=development
LOG_LEVEL=debug
STRICT_MODE=false

# Optional: Database or Persistence (future)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=iaa_db
# DB_USER=user
# DB_PASSWORD=password
